            info += "<a ref='nofollow' href='http://campagne350burundi.wordpress.com/'>Go to group website</a>";
       
       RAH.event_locations.push({
            lat: "-3.376217",
            lon: "29.359349",
            "info": info
        });
    
        var info = "<b>350 Long Island</b><br/> long island<br/>";
       
